@using EntityLayer.Concrete
@model List<Desnination>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_UserNewLayout.cshtml";
}

<h1>Aktif Tur Rotalarımız</h1>
<table class="table table-bordered">
    <tr>
        <th>#</th>
        <th>Şehir</th>
        <th>Süre</th>
        <th>Kapasite</th>
        <th>Fiyat</th>
        <td>Açıklama</td>
        <th>Detaylar</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.DesninationId</td>
            <td>@item.City</td>
            <td>@item.DayNight</td>
            <td>@item.Capacity</td>
            <td>@item.Price</td>
            <td>@item.Description</td>
            <td><a href="#" class="btn btn-outline-info">Detaylar</a></td>
        </tr>
        
    }

</table>

